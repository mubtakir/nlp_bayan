function example3_ColorfulWave(): GeneralShapeEquation {
    /**
     * Ø±Ø³Ù… Ù…ÙˆØ¬Ø© Ø¬ÙŠØ¨ÙŠØ© Ù…Ù„ÙˆÙ†Ø©
     * Draw colorful sine wave
     */
    console.log("\nã€°ï¸ Ù…Ø«Ø§Ù„ 3: Ù…ÙˆØ¬Ø© Ù…Ù„ÙˆÙ†Ø©");
    console.log("â”".repeat(50));
    
    let wave = new GeneralShapeEquation("Ù…ÙˆØ¬Ø© Ù…Ù„ÙˆÙ†Ø©", ShapeType.SINE_WAVE);
    
