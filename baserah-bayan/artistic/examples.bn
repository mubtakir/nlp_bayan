// Example 4: Draw Star
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function example4_Star(): GeneralShapeEquation {
    /**
     * Ø±Ø³Ù… Ù†Ø¬Ù…Ø© Ø®Ù…Ø§Ø³ÙŠØ©
     * Draw five-pointed star
     */
    console.log("\nâ­ Ù…Ø«Ø§Ù„ 4: Ù†Ø¬Ù…Ø©");
    console.log("â”".repeat(50));
    
    let star = new GeneralShapeEquation("Ù†Ø¬Ù…Ø©", ShapeType.POLYGON);
    
    // Ø­Ø³Ø§Ø¨ Ù†Ù‚Ø§Ø· Ø§Ù„Ù†Ø¬Ù…Ø©
    let centerX = 400;
    let centerY = 300;
    let outerRadius = 100;
    let innerRadius = 40;
    let points = 5;
    
    let starPoints = [];
    for (let i = 0; i < points * 2; i++) {
        let angle = (i * Math.PI) / points - Math.PI / 2;
        let radius = i % 2 == 0 ? outerRadius : innerRadius;
        let x = centerX + radius * Math.cos(angle);
        let y = centerY + radius * Math.sin(angle);
        starPoints.push([x, y]);
    }
    
    // Ø±Ø³Ù… Ø®Ø·ÙˆØ· Ø§Ù„Ù†Ø¬Ù…Ø©
    for (let i = 0; i < starPoints.length; i++) {
        let nextI = (i + 1) % starPoints.length;
        star.addLineTerm(
            starPoints[i][0], starPoints[i][1],
            starPoints[nextI][0], starPoints[nextI][1]
        );
    }
    
    // Ø§Ù„Ø£Ù†Ù…Ø§Ø·
    star.setStrokeColor(255, 215, 0, 1);  // Ø°Ù‡Ø¨ÙŠ
    star.setFillColor(255, 255, 0, 0.3);  // Ø£ØµÙØ± Ø´ÙØ§Ù
    star.setLineWidth(3);
    
    star.setState("points", points);
    star.tags = ["star", "polygon", "geometric"];
    star.category = "shapes";
    
    console.log(`âœ… ØªÙ… Ø¥Ù†Ø´Ø§Ø¡: ${star.shapeName}`);
    console.log(`   Ø¹Ø¯Ø¯ Ø§Ù„Ø®Ø·ÙˆØ·: ${star.mathematicalTerms.length}`);
    
    return star;
}

